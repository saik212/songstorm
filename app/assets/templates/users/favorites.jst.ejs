<div class="content-main group" id="likes-main">
	<section class="playlists-section" id="likes-section">
		<%likes.forEach(function(like){ %>
			<div class="liked-song">
				<img class="song-thumb" src="<%= like.escape("song_image") %>" width="100" alt="">
				<h1><a href="#/songs/<%=like.escape('song_id')%>"><%= like.escape('song_title')%></a></h1>
			</div>
		<%}) %>
	</section>
</div>